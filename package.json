{
  "name": "betalife",
  "version": "0.0.1",
  "description": "A boilerplate for starting js code",
  "keywords": [
    "betalife",
    "boilerplate"
  ],
  "private": false,
  "main": "./src/index.js",
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "lint": "pretty-quick --staged && lint-staged",
    "test": "jest",
    "test:cover": "jest --coverage",
    "build": "babel src --out-dir dist",
    "start": "npm run build & node dist/index.js",
    "dev": "nodemon --exec babel-node src/index.js"
  },
  "author": {
    "name": "Build For SDG",
    "email": "buildforsdg@andela.com"
  },
  "homepage": "https://github.com/BuildForSDG/betalife#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/BuildForSDG/betalife.git"
  },
  "bugs": {
    "url": "https://github.com/BuildForSDG/betalife/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^5.15.5",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "helmet": "^3.22.0",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "prettier": "^2.0.5",
    "sequelize": "^5.21.10",
    "sequelize-cli": "^5.5.1",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@types/jest": "^25.2.3",
    "babel-jest": "^25.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.2.4",
    "nodemon": "^2.0.4",
    "pretty-quick": "^2.0.1"
  },
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  }
}
